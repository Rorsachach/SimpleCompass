<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.xiaomicompass">

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/compass_transparent_ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/compass_transparent_ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/newStryle">
        <activity android:name=".CompassCameraActivity"></activity>
        <activity android:name=".SpiritLevelActivit" />
        <activity android:name=".CompassActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>